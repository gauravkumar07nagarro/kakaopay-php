{
    "name": "se468/kakaopay-php",
    "description": "Starting point for Laravel package development.",
    "keywords": ["laravel", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Seyong Cho",
            "email": "se468@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "^6.3",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.2|~1.0.0",
        "phpunit/phpunit" : "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "se468\\Kakaopay\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "se468\\Kakaopay\\KakaopayServiceProvider"
            ]
        }
    }
}
